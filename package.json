{
  "name": "schemes",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "npm run build-css && npm run build-js && npm run copy-assets",
    "build-css": "sass --style compressed node_modules/govuk-frontend/govuk/all.scss schemes/static/govuk-frontend.min.css",
    "build-js": "terser node_modules/govuk-frontend/govuk/all.js --source-map includeSources --output schemes/static/govuk-frontend.min.js",
    "copy-assets": "cp -r node_modules/govuk-frontend/govuk/assets schemes/static/assets"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "govuk-frontend": "^4.7.0"
  },
  "devDependencies": {
    "sass": "^1.67.0",
    "terser": "^5.19.4"
  },
  "engines": {
    "node": "18",
    "npm": "9"
  }
}
