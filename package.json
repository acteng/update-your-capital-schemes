{
  "name": "schemes",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "govuk-frontend:build-css": "sass --style compressed schemes/views/assets/govuk-frontend.scss schemes/views/static/govuk-frontend/govuk-frontend.min.css",
    "govuk-frontend:build-js": "terser node_modules/govuk-frontend/govuk/all.js --source-map includeSources --output schemes/views/static/govuk-frontend/govuk-frontend.min.js",
    "govuk-frontend:copy-assets": "cp -rT node_modules/govuk-frontend/govuk/assets schemes/views/static/govuk-frontend/assets",
    "govuk-frontend:build": "npm run govuk-frontend:build-css && npm run govuk-frontend:build-js && npm run govuk-frontend:copy-assets",
    "govuk-one-login-service-header:build-css": "sass --style compressed --load-path=node_modules/govuk-frontend schemes/views/assets/govuk-one-login-service-header.scss schemes/views/static/govuk-one-login-service-header/govuk-one-login-service-header.min.css",
    "govuk-one-login-service-header:build-js": "terser node_modules/govuk-one-login-service-header/dist/scripts/service-header.js node_modules/govuk-one-login-service-header/dist/scripts/init-service-header.js --source-map includeSources --output schemes/views/static/govuk-one-login-service-header/govuk-one-login-service-header.min.js",
    "govuk-one-login-service-header:build": "npm run govuk-one-login-service-header:build-css && npm run govuk-one-login-service-header:build-js",
    "build": "npm run govuk-frontend:build && npm run govuk-one-login-service-header:build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "govuk-frontend": "^4.7.0",
    "govuk-one-login-service-header": "github:alphagov/di-govuk-one-login-service-header#25cb2d00881048031c6b4a0023b6aef429fcffb3"
  },
  "devDependencies": {
    "sass": "^1.69.0",
    "terser": "^5.26.0"
  },
  "engines": {
    "node": "20",
    "npm": "10"
  }
}
